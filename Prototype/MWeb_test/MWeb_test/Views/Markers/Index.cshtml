@model IEnumerable<MWeb_test.Models.Markers>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MarkerLat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MarkerLng)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Photo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhotoPath)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MarkerLat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MarkerLng)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Photo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhotoPath)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.UserEmail)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MarkerId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MarkerId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MarkerId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
